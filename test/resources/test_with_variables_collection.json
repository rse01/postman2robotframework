{
	"info": {
		"_postman_id": "e8dd96ba-28ab-40e3-bfbc-70527247513c",
		"name": "newmanTest",
		"description": "Makes four requests to HTTPBin (httpbin.org) and tests Newman",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "GET with URL Params",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "d43410bb-70c6-44e4-81a2-0291293dc83b",
						"exec": [
							"var data = JSON.parse(responseBody);",
							"tests[\"Response contains params\"] = data.args.lol === \"true\";"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://httpbin.org/get?lol={{param}}&haha={{param2}}",
					"protocol": "http",
					"host": [
						"httpbin",
						"org"
					],
					"path": [
						"get"
					],
					"query": [
						{
							"key": "lol",
							"value": "{{param}}"
						},
						{
							"key": "haha",
							"value": "{{param2}}"
						}
					]
				},
				"description": "Simple GET request with URL Parameters"
			},
			"response": []
		},
		{
			"name": "POST with JSON body",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "1280213b-c2d6-4278-91cb-52cf6fa42b89",
						"exec": [
							"var data = JSON.parse(responseBody);",
							"tests[\"Check POST param in response\"] = data.json.something === \"cool\";"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
